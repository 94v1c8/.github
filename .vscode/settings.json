{
  "json.schemas": [
    {
      "fileMatch": [
        ".github/ISSUE_TEMPLATE/config.yml"
      ],
      "url": "https://json.schemastore.org/github-issue-config.json"
    },
    {
      "fileMatch": [
        ".github/ISSUE_TEMPLATE/*.yml"
      ],
